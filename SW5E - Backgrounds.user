  <thing id="btPRJens01" name="Judge" description="I ignore the rhetoric a person spouts when meeting them, preferring to judge them by action." compset="BackPerTrt" uniqueness="unique">
    <tag group="Background" tag="bckPRJensaarai"/>
    <tag group="Roll" tag="1"/>
    </thing>
  <thing id="btPRJens02" name="Non-Lethal" description="I&apos;m the first to swing a punch, but the last to draw a weapon." compset="BackPerTrt" uniqueness="unique">
    <tag group="Background" tag="bckPRJensaarai"/>
    <tag group="Roll" tag="2"/>
    </thing>
  <thing id="btPRJens03" name="Danger" description="Danger is my middle name." compset="BackPerTrt" uniqueness="unique">
    <tag group="Background" tag="bckPRJensaarai"/>
    <tag group="Roll" tag="3"/>
    </thing>
  <thing id="btPRJens04" name="Goodwill" description="Goodwill is in short supply in the galaxy, and I always try to foster a little more wherever I go." compset="BackPerTrt" uniqueness="unique">
    <tag group="Background" tag="bckPRJensaarai"/>
    <tag group="Roll" tag="4"/>
    </thing>
  <thing id="btPRJens05" name="Independent" description="Both the Republic and the Empire have tried to crush us under their bootheels. I refuse to yield." compset="BackPerTrt" uniqueness="unique">
    <tag group="Background" tag="bckPRJensaarai"/>
    <tag group="Roll" tag="5"/>
    </thing>
  <thing id="btPRJens06" name="Defiant" description="Defiant to the last isn&apos;t good enough. Upon death I will become a force spirit and continue to defy my killer." compset="BackPerTrt" uniqueness="unique">
    <tag group="Background" tag="bckPRJensaarai"/>
    <tag group="Roll" tag="6"/>
    </thing>
  <thing id="btPRJens07" name="Renegade" description="Well-behaved people rarely make history." compset="BackPerTrt" uniqueness="unique">
    <tag group="Background" tag="bckPRJensaarai"/>
    <tag group="Roll" tag="7"/>
    </thing>
  <thing id="btPRJens08" name="Win" description="Well-behaved people rarely make history." compset="BackPerTrt" uniqueness="unique">
    <tag group="Background" tag="bckPRJensaarai"/>
    <tag group="Roll" tag="8"/>
    </thing>
  <thing id="btPRJensId01" name="Respect" description="Respect will be shown to everyone, disrespect will be shown to those who earn it. (Lawful)" compset="BackIdeal" uniqueness="unique">
    <tag group="Background" tag="bckPRJensaarai"/>
    <tag group="Roll" tag="1"/>
    </thing>
  <thing id="btPRJensId02" name="Freedom" description="People should be free to make their own way without interference. (Chaotic)" compset="BackIdeal" uniqueness="unique">
    <tag group="Background" tag="bckPRJensaarai"/>
    <tag group="Roll" tag="2"/>
    </thing>
  <thing id="btPRJensId03" name="Growth" description="Self-improvement is an unending chore, but one that must be done. (Any)" compset="BackIdeal" uniqueness="unique">
    <tag group="Background" tag="bckPRJensaarai"/>
    <tag group="Roll" tag="3"/>
    </thing>
  <thing id="btPRJensId04" name="Escalation" description="I will immediately end a hostile situation with all means at my disposal. (Neutral)" compset="BackIdeal" uniqueness="unique">
    <tag group="Background" tag="bckPRJensaarai"/>
    <tag group="Roll" tag="4"/>
    </thing>
  <thing id="btPRJensId05" name="Power" description="Given our luck with both Jedi and Sith, I need all I can get. (Dark)" compset="BackIdeal" uniqueness="unique">
    <tag group="Background" tag="bckPRJensaarai"/>
    <tag group="Roll" tag="5"/>
    </thing>
  <thing id="btPRJensId06" name="Redemption" description="There&apos;s a spark of good in everyone. (Light)" compset="BackIdeal" uniqueness="unique">
    <tag group="Background" tag="bckPRJensaarai"/>
    <tag group="Roll" tag="6"/>
    </thing>
  <thing id="btPRJensB01" name="Jensaarai" description="The Jensaarai took me in when nobody else would. I back the order 100%." compset="BackBond" uniqueness="unique">
    <tag group="Background" tag="bckPRJensaarai"/>
    <tag group="Roll" tag="1"/>
    </thing>
  <thing id="btPRJensB02" name="Disillusioned" description="I was once a Jedi, but grew disillusioned by their inactivity." compset="BackBond" uniqueness="unique">
    <tag group="Background" tag="bckPRJensaarai"/>
    <tag group="Roll" tag="2"/>
    </thing>
  <thing id="btPRJensB03" name="Helpless" description="I fight for those who cannot fight for themselves." compset="BackBond" uniqueness="unique">
    <tag group="Background" tag="bckPRJensaarai"/>
    <tag group="Roll" tag="3"/>
    </thing>
  <thing id="btPRJensB04" name="Vengeance" description="I lost my mentor in battle. I will honor their sacrifice by destroying their killer." compset="BackBond" uniqueness="unique">
    <tag group="Background" tag="bckPRJensaarai"/>
    <tag group="Roll" tag="4"/>
    </thing>
  <thing id="btPRJensB05" name="Corruption" description="I was able to slough off Dark Side corruption, but I couldn&apos;t avoid making amends." compset="BackBond" uniqueness="unique">
    <tag group="Background" tag="bckPRJensaarai"/>
    <tag group="Roll" tag="5"/>
    </thing>
  <thing id="btPRJensB06" name="Help" description="The powerful use their strength to raise people up, not keep them down." compset="BackBond" uniqueness="unique">
    <tag group="Background" tag="bckPRJensaarai"/>
    <tag group="Roll" tag="6"/>
    </thing>
  <thing id="btPRJensF01" name="Sadist" description="I might be a little too enamored with force choking my enemies." compset="BackFlaw" uniqueness="unique">
    <tag group="Background" tag="bckPRJensaarai"/>
    <tag group="Roll" tag="1"/>
    </thing>
  <thing id="btPRJensF02" name="Self-Critic" description="I am my own worst critic, in everything I do." compset="BackFlaw" uniqueness="unique">
    <tag group="Background" tag="bckPRJensaarai"/>
    <tag group="Roll" tag="2"/>
    </thing>
  <thing id="btPRJensF03" name="Armor" description="My armor is precision crafted and has served me faithfully. I never give up a chance to prove how well." compset="BackFlaw" uniqueness="unique">
    <tag group="Background" tag="bckPRJensaarai"/>
    <tag group="Roll" tag="3"/>
    </thing>
  <thing id="btPRJensF04" name="Lightsabers" description="I have a collection of lightsabers from fallen foes, and I love adding to it." compset="BackFlaw" uniqueness="unique">
    <tag group="Background" tag="bckPRJensaarai"/>
    <tag group="Roll" tag="4"/>
    </thing>
  <thing id="btPRJensF05" name="Tool" description="The Jedi and Sith worship the Force as a religion. I do everything I can to remind them they&apos;re just worshipping a hammer." compset="BackFlaw" uniqueness="unique">
    <tag group="Background" tag="bckPRJensaarai"/>
    <tag group="Roll" tag="5"/>
    </thing>
  <thing id="btPRJensF06" name="Limits" description="The minute you tell me that someone should not be crossed, I&apos;m already crossing them." compset="BackFlaw" uniqueness="unique">
    <tag group="Background" tag="bckPRJensaarai"/>
    <tag group="Roll" tag="6"/>
    </thing>
  <thing id="bckPRLaborer" name="Laborer" description="You are an experienced laborer and come from a humble social rank. You work with your hands, and anything worth doing is worth doing well. Your life is full of hard work and too little respect for your efforts, but you share a deep camaraderie with your fellow workers." compset="Background" uniqueness="unique">
