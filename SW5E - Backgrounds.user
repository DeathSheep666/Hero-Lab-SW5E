    <tag group="Roll" tag="5"/>
    </thing>
  <thing id="btPRFarTF06" name="Beauty" description="I have a weakness for the exotic beauty of the people of these lands." compset="BackFlaw" uniqueness="unique">
    <tag group="Background" tag="bckPRFarTraveler"/>
    <tag group="Roll" tag="6"/>
    </thing>
