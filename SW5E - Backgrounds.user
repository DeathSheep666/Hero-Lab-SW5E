    <tag group="Roll" tag="5"/>
    </thing>
  <thing id="btPRFarTB06" name="Loved Ones" description="Though I had no choice, I lament having to leave my loved one(s) behind. I hope to see them again one day." compset="BackBond" uniqueness="unique">
