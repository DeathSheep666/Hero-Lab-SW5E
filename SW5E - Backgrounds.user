    <tag group="Roll" tag="4"/>
    </thing>
  <thing id="btPRFarTId05" name="Inquisitive" description="Everything is new, but I have a thirst to learn. (Neutral)" compset="BackIdeal" uniqueness="unique">
    <tag group="Background" tag="bckPRFarTraveler"/>
    <tag group="Roll" tag="5"/>
    </thing>
  <thing id="btPRFarTId06" name="Suspicious" description="I must be careful, for I have no way of telling friend from foe here. (Any)" compset="BackIdeal" uniqueness="unique">
