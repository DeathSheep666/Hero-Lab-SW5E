    <tag group="Roll" tag="3"/>
    </thing>
  <thing id="btPRFarTId04" name="Cunning" description="Though I may not know their ways, neither do they know mine, which can be to my advantage. (Dark)" compset="BackIdeal" uniqueness="unique">
