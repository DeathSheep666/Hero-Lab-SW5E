    <tag group="Roll" tag="6"/>
    </thing>
  <thing id="btPRFarTB01" name="Token" description="So long as I have this token from my homeland, I can face any adversity in this strange land." compset="BackBond" uniqueness="unique">
    <tag group="Background" tag="bckPRFarTraveler"/>
    <tag group="Roll" tag="1"/>
    </thing>
  <thing id="btPRFarTB02" name="Gods" description="The gods of my people are a comfort to me so far from home." compset="BackBond" uniqueness="unique">
    <tag group="Background" tag="bckPRFarTraveler"/>
    <tag group="Roll" tag="2"/>
    </thing>
  <thing id="btPRFarTB03" name="Service" description="I hold no greater cause than my service to my people." compset="BackBond" uniqueness="unique">
    <tag group="Background" tag="bckPRFarTraveler"/>
    <tag group="Roll" tag="3"/>
    </thing>
  <thing id="btPRFarTB04" name="Freedom" description="My freedom is my most precious possession. I&apos;ll never let anyone take it from me again." compset="BackBond" uniqueness="unique">
    <tag group="Background" tag="bckPRFarTraveler"/>
    <tag group="Roll" tag="4"/>
    </thing>
  <thing id="btPRFarTB05" name="Beauty" description="I&apos;m fascinated by the beauty and wonder of this new land." compset="BackBond" uniqueness="unique">
