    <tag group="Roll" tag="1"/>
    </thing>
  <thing id="btPRFarTId02" name="Reserved" description="As someone new to these strange lands, I am cautious and respectful in my dealings. (Lawful)" compset="BackIdeal" uniqueness="unique">
