    <tag group="Background" tag="bckPRHoloTech"/>
    <tag group="Roll" tag="3"/>
    </thing>
  <thing id="btPRHolo04" name="Snuff" description="I like to record people dying and upload it to the holonet." compset="BackPerTrt" uniqueness="unique">
    <tag group="Background" tag="bckPRHoloTech"/>
    <tag group="Roll" tag="4"/>
    </thing>
  <thing id="btPRHolo05" name="Skeptical" description="There&apos;s no such thing as &apos;real news&apos;." compset="BackPerTrt" uniqueness="unique">
    <tag group="Background" tag="bckPRHoloTech"/>
    <tag group="Roll" tag="5"/>
    </thing>
  <thing id="btPRHolo06" name="Information" description="Information is a weapon that I&apos;ve mastered." compset="BackPerTrt" uniqueness="unique">
    <tag group="Background" tag="bckPRHoloTech"/>
    <tag group="Roll" tag="6"/>
    </thing>
  <thing id="btPRHolo07" name="Learning" description="I am always interested in learning new things." compset="BackPerTrt" uniqueness="unique">
    <tag group="Background" tag="bckPRHoloTech"/>
    <tag group="Roll" tag="7"/>
    </thing>
  <thing id="btPRHolo08" name="Direct" description="I prefer interacting indirectly." compset="BackPerTrt" uniqueness="unique">
    <tag group="Background" tag="bckPRHoloTech"/>
    <tag group="Roll" tag="8"/>
    </thing>
  <thing id="btPRHoloId01" name="Duty" description="It is my duty to keep all people in contact so that everything will continue working as intended. (Lawful)" compset="BackIdeal" uniqueness="unique">
    <tag group="Background" tag="bckPRHoloTech"/>
    <tag group="Roll" tag="1"/>
    </thing>
  <thing id="btPRHoloId02" name="Autonomy" description="Information is meant to confuse and overwhelm. I choose to control my own mind by controlling what information I have access to. (Chaotic)" compset="BackIdeal" uniqueness="unique">
    <tag group="Background" tag="bckPRHoloTech"/>
    <tag group="Roll" tag="2"/>
    </thing>
  <thing id="btPRHoloId03" name="Happiness" description="The more hope and joy I spread, the more laughs I bring; that is why I do what I do. (Light)" compset="BackIdeal" uniqueness="unique">
    <tag group="Background" tag="bckPRHoloTech"/>
    <tag group="Roll" tag="3"/>
    </thing>
  <thing id="btPRHoloId04" name="Popularity" description="I want to become the most renown being in all the galaxy, and I&apos;ll destroy the competition. (Dark)" compset="BackIdeal" uniqueness="unique">
    <tag group="Background" tag="bckPRHoloTech"/>
    <tag group="Roll" tag="4"/>
    </thing>
  <thing id="btPRHoloId05" name="Knowledge" description="I don&apos;t care who&apos;s ideology is right, I care who&apos;s ideology will let me learn and document information. (Neutral)" compset="BackIdeal" uniqueness="unique">
    <tag group="Background" tag="bckPRHoloTech"/>
    <tag group="Roll" tag="5"/>
    </thing>
  <thing id="btPRHoloId06" name="Self-Fulfillment" description="I&apos;m just here for the fun of it, no matter what happens it&apos;ll make a great story one day. (Any)" compset="BackIdeal" uniqueness="unique">
    <tag group="Background" tag="bckPRHoloTech"/>
    <tag group="Roll" tag="6"/>
    </thing>
  <thing id="btPRHoloB01" name="Censorship" description="I will do anything to free people from censorship." compset="BackBond" uniqueness="unique">
    <tag group="Background" tag="bckPRHoloTech"/>
    <tag group="Roll" tag="1"/>
    </thing>
  <thing id="btPRHoloB02" name="Crime Lord" description="I can&apos;t let anyone know I work for a crime lord." compset="BackBond" uniqueness="unique">
    <tag group="Background" tag="bckPRHoloTech"/>
    <tag group="Roll" tag="2"/>
    </thing>
  <thing id="btPRHoloB03" name="Ideology" description="I see truth in an ideology, and I must spread that ideal to all." compset="BackBond" uniqueness="unique">
    <tag group="Background" tag="bckPRHoloTech"/>
    <tag group="Roll" tag="3"/>
    </thing>
  <thing id="btPRHoloB04" name="Platform" description="I got a man killed by giving him a platform to speak, I can&apos;t let that happen again." compset="BackBond" uniqueness="unique">
    <tag group="Background" tag="bckPRHoloTech"/>
    <tag group="Roll" tag="4"/>
    </thing>
  <thing id="btPRHoloB05" name="Government" description="I&apos;m on the run from a government organization that I spread propaganda against and who&apos;s propaganda I destroy." compset="BackBond" uniqueness="unique">
    <tag group="Background" tag="bckPRHoloTech"/>
    <tag group="Roll" tag="5"/>
    </thing>
  <thing id="btPRHoloB06" name="Logs" description="I have a famous person&apos;s private logs, and I don&apos;t know what to do with them." compset="BackBond" uniqueness="unique">
    <tag group="Background" tag="bckPRHoloTech"/>
    <tag group="Roll" tag="6"/>
    </thing>
  <thing id="btPRHoloF01" name="Doubting" description="I tend to think any broadcaster is a fraud immediately unless they prove otherwise." compset="BackFlaw" uniqueness="unique">
    <tag group="Background" tag="bckPRHoloTech"/>
    <tag group="Roll" tag="1"/>
    </thing>
  <thing id="btPRHoloF02" name="Repeat" description="I tend to repeat the same mistake unless it is pointed out to me." compset="BackFlaw" uniqueness="unique">
    <tag group="Background" tag="bckPRHoloTech"/>
    <tag group="Roll" tag="2"/>
    </thing>
  <thing id="btPRHoloF03" name="Ramble" description="I will ramble endlessly about a random holonet broadcast that I saw which I think was the best ever." compset="BackFlaw" uniqueness="unique">
    <tag group="Background" tag="bckPRHoloTech"/>
    <tag group="Roll" tag="3"/>
    </thing>
  <thing id="btPRHoloF04" name="Drone" description="I think everyone who works a normal job is a drone without the ability to think freely." compset="BackFlaw" uniqueness="unique">
    <tag group="Background" tag="bckPRHoloTech"/>
    <tag group="Roll" tag="4"/>
    </thing>
  <thing id="btPRHoloF05" name="References" description="I describe everything using references to obscure holonet recordings." compset="BackFlaw" uniqueness="unique">
    <tag group="Background" tag="bckPRHoloTech"/>
    <tag group="Roll" tag="5"/>
    </thing>
  <thing id="btPRHoloF06" name="Desensitized" description="I&apos;ve become completely desensitized to violence and graphic imagery, to the point where I laugh in an off putting way whenever I see it." compset="BackFlaw" uniqueness="unique">
    <tag group="Background" tag="bckPRHoloTech"/>
    <tag group="Roll" tag="6"/>
    </thing>
  <thing id="bckPRImpKnight" name="Imperial Knight" description="You are an Imperial Knight, a Force-wielding protector of the Fel Empire. Your order combines the teachings of the Jedi with the creed of the Royal Guard of old, using the Force as a powerful and respected tool with which to serve the Force as embodied by your Emperor. Imperial Knights do not share the philosophies of the Jedi with regards to the Force and its light side, and are well aware of the dangers of the dark side, dutifully rejecting its use and temptations.\n\nWhile Imperial Knights wield and construct their own lightsabers, they assign them no ceremonial value, treating them as nothing more than tools. As such, almost all lightsabers of the Imperial Knights look the same, with uniform hilt design and signature silver blades. These blades are produced by synthetic crystals similar to the red variety used by the Sith, created with knowledge from Emperor Palpatine’s {i}Book of Anger{/i}, but in a process bereft of the dark side. Imperial Knights are also recognized by their crimson armor emblazoned with the insignia of the Empire and ideally crafted of lightsaber-resistant materials." compset="Background" uniqueness="unique">
    <fieldval field="StartGear" value="a revised copy of the *Book of Anger*, an Imperial insignia, a black cape with a hood, and a belt pouch containing 100 cr."/>
    <fieldval field="StartAll" value="a revised copy of the *Book of Anger*, an Imperial insignia, a black cape with a hood, and a belt pouch containing 100 cr."/>
    <fieldval field="cSkillMax" value="2"/>
    <tag group="AllowSkl1" tag="skAthletic"/>
    <tag group="AllowSkl1" tag="skIntim"/>
    <tag group="AllowSkl1" tag="skPercep"/>
    <tag group="AllowSkl1" tag="skPRPiloting"/>
    <bootstrap thing="abPRWillEmp"></bootstrap>
    <bootstrap thing="cfgPRImpKntFeat"></bootstrap>
    </thing>
  <thing id="btPRImpKn01" name="Crisis" description="If a crisis comes, my place will be at the Emperor&apos;s side." compset="BackPerTrt" uniqueness="unique">
    <tag group="Background" tag="bckPRImpKnight"/>
    <tag group="Roll" tag="1"/>
    </thing>
  <thing id="btPRImpKn02" name="Title" description="I dislike my title. It is only there to make me seem more important than I am." compset="BackPerTrt" uniqueness="unique">
    <tag group="Background" tag="bckPRImpKnight"/>
    <tag group="Roll" tag="2"/>
    </thing>
  <thing id="btPRImpKn03" name="Adviser" description="I do not lead. I only advise." compset="BackPerTrt" uniqueness="unique">
    <tag group="Background" tag="bckPRImpKnight"/>
    <tag group="Roll" tag="3"/>
    </thing>
  <thing id="btPRImpKn04" name="Greater Good" description="Some things are more important than the life of any one person�including mine." compset="BackPerTrt" uniqueness="unique">
    <tag group="Background" tag="bckPRImpKnight"/>
    <tag group="Roll" tag="4"/>
    </thing>
  <thing id="btPRImpKn05" name="Inflexible" description="I will not bend, I will not break, I will not serve." compset="BackPerTrt" uniqueness="unique">
    <tag group="Background" tag="bckPRImpKnight"/>
    <tag group="Roll" tag="5"/>
    </thing>
  <thing id="btPRImpKn06" name="Calm" description="I will not strike an enemy down in anger." compset="BackPerTrt" uniqueness="unique">
    <tag group="Background" tag="bckPRImpKnight"/>
    <tag group="Roll" tag="6"/>
    </thing>
  <thing id="btPRImpKn07" name="Reliable" description="I can do whatever needs to be done!" compset="BackPerTrt" uniqueness="unique">
    <tag group="Background" tag="bckPRImpKnight"/>
    <tag group="Roll" tag="7"/>
    </thing>
  <thing id="btPRImpKn08" name="Test" description="I&apos;ve wanted to test myself against the Sith!" compset="BackPerTrt" uniqueness="unique">
    <tag group="Background" tag="bckPRImpKnight"/>
    <tag group="Roll" tag="8"/>
    </thing>
  <thing id="btPRImpKnId01" name="Duty" description="We serve with honor and if we are asked to sacrifice our honor as our duty, then we are betrayed. (Lawful)" compset="BackIdeal" uniqueness="unique">
    <tag group="Background" tag="bckPRImpKnight"/>
    <tag group="Roll" tag="1"/>
    </thing>
  <thing id="btPRImpKnId02" name="Loyalty" description="I do not understand, sire. But I will obey. (Lawful)" compset="BackIdeal" uniqueness="unique">
    <tag group="Background" tag="bckPRImpKnight"/>
    <tag group="Roll" tag="2"/>
    </thing>
  <thing id="btPRImpKnId03" name="Faith" description="I will not stray from the true path. I feel the responsibility that comes with wielding the Force now more than ever. (Light)" compset="BackIdeal" uniqueness="unique">
    <tag group="Background" tag="bckPRImpKnight"/>
    <tag group="Roll" tag="3"/>
    </thing>
  <thing id="btPRImpKnId04" name="Prudence" description="Where I come from, we at least hear someone out before we shoot them. (Neutral)" compset="BackIdeal" uniqueness="unique">
    <tag group="Background" tag="bckPRImpKnight"/>
    <tag group="Roll" tag="4"/>
    </thing>
  <thing id="btPRImpKnId05" name="Pride" description="We are Imperial Knights. There are none better. (Any)" compset="BackIdeal" uniqueness="unique">
    <tag group="Background" tag="bckPRImpKnight"/>
    <tag group="Roll" tag="5"/>
    </thing>
  <thing id="btPRImpKnId06" name="Strategy" description="A tiny fever wasp can carry death into places where a Krayt dragon cannot go. (Any)" compset="BackIdeal" uniqueness="unique">
    <tag group="Background" tag="bckPRImpKnight"/>
    <tag group="Roll" tag="6"/>
    </thing>
  <thing id="btPRImpKnB01" name="Emperor" description="Our duty is to the Force as embodied by the Emperor." compset="BackBond" uniqueness="unique">
    <tag group="Background" tag="bckPRImpKnight"/>
    <tag group="Roll" tag="1"/>
    </thing>
  <thing id="btPRImpKnB02" name="Individual" description="There is a person I care more about than my vows as a Knight or my loyalty to the Empire." compset="BackBond" uniqueness="unique">
    <tag group="Background" tag="bckPRImpKnight"/>
    <tag group="Roll" tag="2"/>
    </thing>
  <thing id="btPRImpKnB03" name="Will" description="I might, by strength of my own will, develop my own creations." compset="BackBond" uniqueness="unique">
    <tag group="Background" tag="bckPRImpKnight"/>
    <tag group="Roll" tag="3"/>
    </thing>
  <thing id="btPRImpKnB04" name="Protect" description="I will not allow another who has risen in my defense to be killed!" compset="BackBond" uniqueness="unique">
    <tag group="Background" tag="bckPRImpKnight"/>
    <tag group="Roll" tag="4"/>
    </thing>
  <thing id="btPRImpKnB05" name="Friend" description="My friend is not the most orthodox of Knights. I&apos;ve learned a lot from her." compset="BackBond" uniqueness="unique">
