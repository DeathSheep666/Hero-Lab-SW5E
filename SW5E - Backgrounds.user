    <tag group="Roll" tag="1"/>
    </thing>
  <thing id="btPRFarT02" name="Cuisine" description="I have my own ideas about food, and I find the eating habits of others fascinating, confusing, or revolting." compset="BackPerTrt" uniqueness="unique">
    <tag group="Background" tag="bckPRFarTraveler"/>
    <tag group="Roll" tag="2"/>
    </thing>
  <thing id="btPRFarT03" name="Code of Honor" description="I have a strong code of honor or sense of propriety that others don&apos;t comprehend." compset="BackPerTrt" uniqueness="unique">
