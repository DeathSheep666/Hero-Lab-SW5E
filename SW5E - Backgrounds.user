    </thing>
  <thing id="btPRFctAd05" name="Tenacious" description="Giving up is never an option for me." compset="BackPerTrt" uniqueness="unique">
    <tag group="Background" tag="bckPRFactAdven"/>
    <tag group="Roll" tag="5"/>
    </thing>
  <thing id="btPRFctAd06" name="Wild" description="I feel more at home in monster-infested dungeons than in civilized society." compset="BackPerTrt" uniqueness="unique">
