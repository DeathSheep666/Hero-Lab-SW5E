  <thing id="btPRFctAdF05" name="Thief" description="My habit of looting dungeons makes me swipe things I shouldn&apos;t." compset="BackFlaw" uniqueness="unique">
    <tag group="Background" tag="bckPRFactAdven"/>
    <tag group="Roll" tag="5"/>
    </thing>
  <thing id="btPRFctAdF06" name="Enemy" description="A monstrous enemy I faced in the past still leaves me quivering with fear." compset="BackFlaw" uniqueness="unique">
