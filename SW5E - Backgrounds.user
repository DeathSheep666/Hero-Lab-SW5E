    <tag group="Roll" tag="6"/>
    </thing>
  <thing id="btPRFctAdB01" name="Artifact" description="I carry with me a strange artifact that I found in a dungeon, but I don&apos;t know what it&apos;s for." compset="BackBond" uniqueness="unique">
