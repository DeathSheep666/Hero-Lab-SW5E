  <thing id="raPRGotalAbInc" name="Gotal Ability Score Increase" description="Your Wisdom score increases by 2, and one other ability score of your choice increases by 1." compset="RaceSpec">
    <fieldval field="usrCandid1" value="component.BaseAttr &amp; (IsAttr.aSTR | IsAttr.aDEX | IsAttr.aCON | IsAttr.aINT | IsAttr.aCHA)"/>
