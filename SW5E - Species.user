  <thing id="raPRHumProfic" name="Proficiency" description="You gain proficiency in one skill, one tool, and one weapon of your choice." compset="RaceSpec">
    <fieldval field="usrCandid1" value="component.BaseWep &amp; (wProfReq.Martial|wProfReq.Simple) &amp; !Helper.Helper"/>
