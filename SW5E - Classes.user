  <thing id="cEngPRSurvImp" name="Surveillance Implant" description="This augmentation replaces your face.\n\nThis implant includes a headcomm with a scrambler that automatically encodes messages sent to a specified recipient commlink or receiver." compset="CustomSpec" uniqueness="unique">
