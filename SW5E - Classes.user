    <tag group="abDuration" tag="Constant"/>
    <tag group="Helper" tag="Tertiary"/>
    <tag group="abCategory" tag="CyberMod" name="Cybertech Modifications" abbrev="Cybertech Modifications"/>
    <tag group="AllowRCust" tag="cfgPRCyberTech"/>
