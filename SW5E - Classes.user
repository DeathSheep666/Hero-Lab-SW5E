    </thing>
  <thing id="cEngPRIntInh" name="Integrated Inhibitor" description="{i}{b}Prerequisite: {/b}5th level,{/i}\n\nWhile using your harness as a tech focus, you gain a +1 bonus to your tech save DC. This bonus increases to +2 at 9th level and +3 at 13th level." compset="CustomSpec" uniqueness="unique">
