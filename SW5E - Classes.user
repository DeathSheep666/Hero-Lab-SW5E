  <thing id="cBerPRJuggApp" name="Juggernaut Approach" description="Certain berserkers find that the strongest offense is a good defense. Berserkers of the Juggernaut Approach wear the heaviest of armor not because they fear injury, but to strike fear in their hapless attackers." compset="CustomSpec" uniqueness="unique">
    <tag group="SpecSource" tag="cHelpBrs"/>
    <tag group="abCategory" tag="BerApps" name="Berserker Approaches" abbrev="Berserker Approaches"/>
    <tag group="Helper" tag="Secondary"/>
