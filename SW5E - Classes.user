  <thing id="cEngPRQckStrtEng" name="Quick Start Engine" description="{i}{b}Prerequisite: {/b}5th level, Jet Pack{/i}\n\nYou can now activate your Jet Pack as an object interaction rather than as a bonus action." compset="CustomSpec" uniqueness="unique">
