      <autotag group="ClSpecWhen" tag="17"/>
      </bootstrap>
    <bootstrap thing="cMonPRAvatar">
      <autotag group="ClSpecWhen" tag="17"/>
      </bootstrap>
    <bootstrap thing="cMonPRElemAdept">
