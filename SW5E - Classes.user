        focus.field[cArrKnSpl].arrayvalue[6] += 7
        focus.field[cArrKnSpl].arrayvalue[7] += 8
        focus.field[cArrKnSpl].arrayvalue[8] += 9
        focus.field[cArrKnSpl].arrayvalue[9] += 10
        focus.field[cArrKnSpl].arrayvalue[10] += 11
        focus.field[cArrKnSpl].arrayvalue[11] += 12
        focus.field[cArrKnSpl].arrayvalue[12] += 13
        focus.field[cArrKnSpl].arrayvalue[13] += 14
        focus.field[cArrKnSpl].arrayvalue[14] += 15
        focus.field[cArrKnSpl].arrayvalue[15] += 16
        focus.field[cArrKnSpl].arrayvalue[16] += 17
        focus.field[cArrKnSpl].arrayvalue[17] += 18
        focus.field[cArrKnSpl].arrayvalue[18] += 19
