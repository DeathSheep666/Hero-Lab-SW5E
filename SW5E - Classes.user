    <eval phase="Level" priority="6000"><![CDATA[
      ~ Don't reduce our capabilities for multiclassing if this is the first
      ~ class we have taken.
      doneif (tagis[Helper.FirstLevel] <> 0)

      ~ Multiclass scholars lose three skills, all weapons, and all save proficiencies
      perform delete[ProfSave.?]
      perform delete[ArmProfGrp.WepSimple]
      perform delete[WepProf.?]

      field[cSkillMax].value -= 3
      field[cToolMax].value-=1]]></eval>
    <eval phase="PreLevel" index="2"><![CDATA[      ~ If we're disabled, do nothing &
 
      doneif (tagis[Helper.Disable] = 1)
      ~ Pull all the simple vibroweapon weapon proficiency tags to ourself
       foreach thing in BaseWep where "wGroup.Vibroweapon & wProfReq.Simple"
           perform eachthing.pulltags[WepProf.?]
       nexteach

      ~ Now push all tags to the hero

      perform hero.pushtags[WepProf.?]]]></eval>
    <eval phase="PreLevel" index="3"><![CDATA[      ~ If we're disabled, do nothing &
 
      doneif (tagis[Helper.Disable] = 1)
      ~ Pull all the simple vibroweapon weapon proficiency tags to ourself
       foreach thing in BaseWep where "wGroup.Blaster & wProfReq.Simple"
           perform eachthing.pulltags[WepProf.?]
       nexteach

      ~ Now push all tags to the hero

      perform hero.pushtags[WepProf.?]]]></eval>
    </thing>
  <thing id="cScholar" name="Scholar" compset="ClassLevel" maxlimit="20">
    <tag group="ClassType" tag="Normal"/>
    <bootstrap thing="cHelpSch"></bootstrap>
