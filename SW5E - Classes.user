    </thing>
  <thing id="cEngPRSharpNoise" name="Sharp Noise" description="As an action while wielding your modified instrument, choose a creature you can see. If it can hear you, it must succeed on a Constitution saving throw or take 1d4 sonic damage and have disadvantage on its next attack roll before the end of its next turn.\n\nThis feature’s damage increases by 1d4 when you reach 5th level (2d4), 11th level (3d4), and 17th level (4d4)." compset="CustomSpec" uniqueness="unique">
    <fieldval field="wDieCount" value="1"/>
    <fieldval field="wDieSize" value="4"/>
    <tag group="abRange" tag="Personal"/>
    <tag group="abDuration" tag="Constant"/>
    <tag group="AllowRCust" tag="cfgPRAudioTech"/>
    <tag group="abAction" tag="Action"/>
    <tag group="abSave" tag="aCON"/>
    <tag group="StandardDC" tag="aINT"/>
    <tag group="Helper" tag="WeaponSpec"/>
    <tag group="DamageType" tag="dtPRSonic"/>
    <tag group="Helper" tag="Tertiary"/>
    <tag group="abCategory" tag="AudioMod" name="Audiotech Modifications" abbrev="Audiotech Modifications"/>
    <eval phase="PostLevel" priority="10000"><![CDATA[      field[abValue].value += herofield[tTotLevel].value

      if (field[abValue].value >= 17) then
       field[wDieCount].value = 4
       field[livename].text = field[name].text & " 4d4"
      elseif (field[abValue].value >= 11) then
       field[wDieCount].value = 3
       field[livename].text = field[name].text & " 3d4"
      elseif (field[abValue].value >= 5) then
       field[wDieCount].value = 2
       field[livename].text = field[name].text & " 2d4"
      else
       field[wDieCount].value = 1
       field[livename].text = field[name].text & " 1d4"
      endif]]></eval>
    </thing>
  <thing id="cEngPRShckMnt" name="Shock Mount" description="{i}{b}Prerequisite: {/b}5th level{/i}\n\nWhile using your instrument as a tech focus, you gain a +1 bonus to your tech save DC. This bonus increases to +2 at 9th level and +3 at 13th level." compset="CustomSpec" uniqueness="unique">
