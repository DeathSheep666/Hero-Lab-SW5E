endif]]></eval>
    </thing>
  <thing id="cGrdPRChShield" name="Channel the Force: Shield" description="When an ally is hit by a weapon attack while within 10 feet of you, you can use your reaction and expend a use of your Channel the Force to attempt to divert the attack. When you do so, the damage the chosen ally takes from the attack is reduced by 1d10 + your Wisdom or Charisma modifier (your choice) + your guardian level.\n\nAt 11th level, the chosen ally must be within 30 feet of you to benefit from this feature. At 17th level, the chosen ally must be within 60 feet." compset="ClSpecial">
    <fieldval field="abRange" value="10"/>
    <tag group="abRange" tag="Feet"/>
    <tag group="abDuration" tag="Instant"/>
    <tag group="abAction" tag="Reaction"/>
    <eval phase="PostLevel" priority="10000"><![CDATA[field[abValue].value = #levelcount[Guardian]
field[abValue2].value = #attrbonus1[aWIS]
field[abValue3].value = #attrbonus1[aCHA]

if (field[abValue].value >= 17) then
     field[abRange].value = 60
elseif (field[abValue].value >= 11) then
     field[abRange].value = 30
else
     field[abRange].value = 10
endif

if (field[abValue2].value >= field[abValue3].value) then
     field[abValue4].value = field[abValue].value + field[abValue2].value
else
     field[abValue4].value = field[abValue].value + field[abValue3].value
endif

field[livename].text = field[thingname].text & " (1d10+" & field[abValue4].value & ")"
field[listname].text = field[thingname].text & " (1d10+" & field[abValue4].value & ")"]]></eval>
    </thing>
  <thing id="cGrdPRAmpAura" name="Amplified Auras" description="Once on each of your turns, you can choose an ally within 10 feet of you. While the chosen ally is within 10 feet of you, your auras emit from that ally as if you were standing in that ally’s space. A creature in the area of more than one instance of your auras is affected only once.\n\nAt 11th level, the chosen ally must be within 30 feet of you to benefit from this feature. At 17th level, the chosen ally must be within 60 feet." compset="ClSpecial">
    <fieldval field="abRange" value="10"/>
