  <thing id="cEngPRArkVect" name="Arkayd Vector" description="While using your wristpad as a tech focus, you gain a +1 bonus to tech attack rolls. This bonus increases to +2 at 9th level and +3 at 13th level." compset="CustomSpec" uniqueness="unique">
