    <tag group="abRange" tag="Personal"/>
    <tag group="abDuration" tag="Instant"/>
    <tag group="AllowRCust" tag="cfgPRGadget"/>
    <tag group="abCategory" tag="GadgetCon"/>
    <eval phase="Render" priority="10000"><![CDATA[field[abValue].value = #levelcount[Engineer]

if (field[abValue].value >= 13) then
   field[livename].text = field[thingname].text & " +3"
elseif (field[abValue].value >= 9) then
   field[livename].text = field[thingname].text & " +2"
else
   field[livename].text = field[thingname].text & " +1"
endif]]></eval>
    <exprreq message="5th level required."><![CDATA[#levelcount[Engineer] >= 5]]></exprreq>
    </thing>
  <thing id="cEngPRIntTarg" name="Integrated Targeter" description="{i}{b}Prerequisite: {/b}5th level,{/i}\n\nWhile using your harness as a tech focus, you gain a +1 bonus to tech attack rolls. This bonus increases to +2 at 9th level and +3 at 13th level." compset="CustomSpec" uniqueness="unique">
