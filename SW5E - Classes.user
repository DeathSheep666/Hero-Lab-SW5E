    </thing>
  <thing id="cEngPRProSupEnh" name="Prototype Support Enhancement" description="{i}{b}Prerequisite: {/b}9th level, Support Song Enhancement{/i}\n\nWhile playing your Song of Support, when you use your Song of Support’s Potent Amplitude feature, the target instead takes no damage if they succeed on the saving throw, and only half damage if they fail." compset="CustomSpec" uniqueness="unique">
    <tag group="abDuration" tag="Constant"/>
    <tag group="AllowRCust" tag="cfgPRAudioTech"/>
