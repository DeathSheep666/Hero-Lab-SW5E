  <thing id="cEnhPRAdvSupEnh" name="Advanced Support Enhancement" description="{i}{b}Prerequisite: {/b}15th level, Prototype Support Enhancement{/i}\n\nWhile playing your Song of Support, allies add your Intelligence modifier to their death saving throws (minimum of +1). If this amount would increase the roll of the d20 to 20 or greater, the creature regains 1 hit point." compset="CustomSpec" uniqueness="unique">
    <tag group="Helper" tag="Tertiary"/>
    <tag group="abCategory" tag="AudioMod" name="Audiotech Modifications" abbrev="Audiotech Modifications"/>
