  <thing id="cXXXPRMakashi" name="Makashi Form" description="Until the start of your next turn, when a creature makes a melee weapon attack against you and misses, you can use your reaction to make one melee weapon attack against that creature." compset="CustomSpec" uniqueness="unique">
    <fieldval field="abDuration" value="1"/>
