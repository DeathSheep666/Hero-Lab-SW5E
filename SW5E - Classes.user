  <thing id="cSchPRDelMov" name="Deliberate Movement" description="You can expend one superiority die to take the Disengage action as a bonus action and ignore the effects of standard difficult terrain until the end of your turn." compset="CustomSpec" uniqueness="unique">
    <fieldval field="abDuration" value="1"/>
    <tag group="Helper" tag="Secondary"/>
