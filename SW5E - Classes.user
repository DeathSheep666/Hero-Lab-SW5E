    <tag group="abDuration" tag="Instant"/>
    <tag group="Usage" tag="LongRest"/>
    <eval phase="PostLevel" priority="10000">field[trkMax].value = field[xCount].value</eval>
    </thing>
  <thing id="cGrdPRUnhinChrg" name="Unhindered Charge" description="Wen you move at least 10 feet before making a melee weapon attack, you deal additional damage equal to your Strength modifier." compset="ClSpecial">
