    <tag group="abCategory" tag="FtrFigStyl"/>
    <tag group="SpecSource" tag="cHelpSct"/>
    <tag group="abCategory" tag="SctFigStyl"/>
    </thing>
  <thing id="cXXXPRShield" name="Shield Style" description="You are skilled at using your shield to defend your allies as well as yourself. While you are wielding a shield with which you are proficient, you gain the following benefits:\n\n- If you are wielding a heavy shield, you are no longer required to wield a weapon with the light property in the other hand.\n- You can use a bonus action to try to shove or trip a creature within 5 feet of you with your shield.\n- When you use a shield as an improvised weapon, you are considered proficient in it." compset="CustomSpec" uniqueness="unique">
    <tag group="SpecSource" tag="cHelpGrd"/>
    <tag group="Helper" tag="Secondary"/>
    <tag group="abCategory" tag="GrdFight"/>
    <tag group="abRange" tag="Personal"/>
    <tag group="abDuration" tag="Constant"/>
