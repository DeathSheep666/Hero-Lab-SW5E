  <thing id="cXXXPRAuraConq" name="Aura of Conquest" description="Whenever a creature who is frightened of you starts its turn within 5 feet of you, its speed is reduced to 0 and that creature takes psychic damage equal to half your guardian level." compset="CustomSpec" uniqueness="unique">
