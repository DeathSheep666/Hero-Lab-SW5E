    <eval phase="PostLevel" priority="10000">field[trkMax].value = hero.childfound[ProfBonus].field[tProfBonus].value</eval>
    </thing>
  <thing id="cSctPRMarkPred" name="Mark of the Predator" description="If the target of your Ranger’s Quarry feature can see you, a number of friendly creatures you choose up to your Intelligence modifier have advantage on Dexterity (Stealth) checks made to hide from it." compset="ClSpecial">
    <tag group="abRange" tag="AttackRan"/>
    <tag group="abDuration" tag="Instant"/>
    <eval phase="PostAttr"><![CDATA[field[abValue].value = #attrbonus[aINT]
field[listname].text = field[thingname].text & " (" & field[abValue].value & " creatures)"
field[livename].text = field[thingname].text & " (" & field[abValue].value & " creatures)"]]></eval>
    </thing>
  <thing id="cSctPRPredReslv" name="Predator&apos;s Resolve" description="You can use your action to gain the following benefits for 1 minute:\n\n- You have advantage on Constitution saving throws.\n- At the start of each of your turns, you gain temporary hit points equal to your Constitution modifier (minimum of one).\n- When you use your Hunting Party feature and the target of the attack is your Ranger’s Quarry, your ally gains a bonus to damage on the attack equal to your Ranger’s Quarry Damage Die.\n\nOnce you’ve used this feature, you can’t use it again until you finish a short or long rest." compset="ClSpecial">
    <fieldval field="trkMax" value="1"/>
    <fieldval field="abDuration" value="1"/>
    <tag group="abAction" tag="None"/>
    <tag group="abRange" tag="Personal"/>
    <tag group="Usage" tag="ShortRest"/>
    <tag group="abDuration" tag="Minute"/>
    <eval phase="PostAttr" priority="10000"><![CDATA[field[abValue].value = #attrbonus1[aCON]
field[livename].text = field[thingname].text & " (" & field[abValue].value & " temporary hp)"
field[listname].text = field[thingname].text & " (" & field[abValue].value & " temporary hp)"]]></eval>
    </thing>
  <thing id="cSctPRPreyWeak" name="Prey on the Weak" description="When you hit a creature with a weapon attack, and the creature is below its hit point maximum, the next attack roll made against that creature before the end of your next turn by someone other than you has advantage." compset="ClSpecial">
    <fieldval field="abDuration" value="1"/>
    <tag group="abRange" tag="Attack"/>
    <tag group="abAction" tag="None"/>
    <tag group="abDuration" tag="Round"/>
    </thing>
  <thing id="cSctPROnTheHunt" name="On the Hunt" description="When you use your Predator’s Resolve feature, a number of friendly creatures you choose up to your Intelligence modifier that you can see within 30 feet of you also gain the benefits of the feature." compset="ClSpecial">
