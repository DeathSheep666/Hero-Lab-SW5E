    <tag group="abDuration" tag="Constant"/>
    </thing>
  <thing id="cEngPRBiochemAmp" name="Biochemist&apos;s Amplifier" description="{i}{b}Prerequisite: {/b}5th level{/i}\n\nWhile using your biochemist’s pack as a tech focus, you gain a +1 bonus to tech attack rolls. This bonus increases to +2 at 9th level and +3 at 13th level." compset="CustomSpec" uniqueness="unique">
