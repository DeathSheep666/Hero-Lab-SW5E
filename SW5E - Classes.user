    <tag group="abAction" tag="Reaction"/>
    <tag group="abRange" tag="Feet"/>
    <tag group="abDuration" tag="Instant"/>
    </thing>
  <thing id="cSctPRImpDecoy" name="Improved Decoys" description="You can create up to four duplicates of yourself, instead of one, when you use your Holographic Decoy feature. When you use your bonus action to move a decoy, you can move any number of them with the same bonus action." compset="ClSpecial"></thing>
  <thing id="cSctPRIllTech" name="Illusionist Technique" description="Among the galaxy you may find the various infiltrators and agents who make ample use of holographic technology to get themselves in and out of danger. Followers of the Illusionist Technique make use of this holographic technology to give themselves an illusory edge in combat." compset="CustomSpec" uniqueness="unique">
    <tag group="SpecSource" tag="cHelpSct"/>
    <tag group="Helper" tag="Primary"/>
    <tag group="abCategory" tag="SctTech"/>
    <bootstrap thing="cSctPRChargedIll">
      <autotag group="ClSpecWhen" tag="7"/>
      </bootstrap>
