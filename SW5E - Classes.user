  <thing id="cOptPRLethBProf" name="Bonus Proficiencies" description="You gain proficiency with the disguise kit and the poisoner’s kit." compset="ClSpecial">
    <tag group="abRange" tag="Personal"/>
    <tag group="abDuration" tag="Constant"/>
    <bootstrap thing="gPRDisguiseKit">
      <autotag group="Helper" tag="TrainTool"/>
