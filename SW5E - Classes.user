  <thing id="cMonPRDiamondSoul" name="Diamond Soul" description="Your mastery of focus grants you proficiency in all saving throws, and when you fail a saving throw, you can spend 1 focus point to reroll it, taking the new roll." compset="ClSpecial" summary="1 focus: reroll a failed saving throw.">
    <tag group="abAction" tag="None" name="No action" abbrev="None"/>
    <tag group="abDuration" tag="Constant" name="Constant" abbrev="cons"/>
    <tag group="abRange" tag="Personal" name="Personal" abbrev="pers"/>
    <eval phase="PostLevel" priority="10000"><![CDATA[
      ~ If we're not shown, just get out now
      doneif (tagis[Helper.ShowSpec] = 0)

      ~ If we're disabled, do nothing
      doneif (tagis[Helper.Disable] <> 0)

      perform hero.assign[ProfSave.svSTR]
      perform hero.assign[ProfSave.svDEX]
      perform hero.assign[ProfSave.svCON]
      perform hero.assign[ProfSave.svINT]
      perform hero.assign[ProfSave.svWIS]
      perform hero.assign[ProfSave.svCHA]]]>
