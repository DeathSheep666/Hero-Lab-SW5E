    <tag group="abRange" tag="HearSight"/>
    <tag group="abDuration" tag="Instant"/>
    <tag group="SpecSource" tag="cHelpSch"/>
    <tag group="abAction" tag="Reaction"/>
    </thing>
  <thing id="cSchPRMeasAct" name="Measured Action" description="As a reaction when you make a roll for a contested skill check, you can expend a superiority die to add to the roll. You can use this maneuver before or after making the contested skill check roll, but before any effects of the contested skill check are determined." compset="CustomSpec" uniqueness="unique">
    <tag group="Helper" tag="Secondary"/>
    <tag group="abCategory" tag="SchManeuvers" name="Scholar Maneuvers" abbrev="Scholar Maneuvers"/>
    <tag group="abRange" tag="Personal"/>
    <tag group="abDuration" tag="Instant"/>
    <tag group="SpecSource" tag="cHelpSch"/>
    <tag group="abAction" tag="Reaction"/>
    </thing>
  <thing id="cSchPROneStpAh" name="One Step Ahead" description="When you roll initiative and you are not surprised, you can expend a superiority die and add the number rolled to your initiative." compset="CustomSpec" uniqueness="unique">
    <tag group="Helper" tag="Secondary"/>
    <tag group="abCategory" tag="SchManeuvers" name="Scholar Maneuvers" abbrev="Scholar Maneuvers"/>
