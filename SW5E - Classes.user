  <thing id="cEngPRIntSubArm" name="Integrated Subdermal Armor" description="{i}{b}Prerequisite: {/b}5th level{/i}\n\nWhen you aren’t wearing armor, your AC becomes 13 + your Dexterity modifier." compset="CustomSpec" uniqueness="unique">
