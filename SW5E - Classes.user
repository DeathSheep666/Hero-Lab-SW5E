    <exprreq message="5th level required."><![CDATA[#levelcount[Engineer] >= 5]]></exprreq>
    </thing>
  <thing id="cEngPRJetPack" name="Jet Pack" description="You integrate a jet pack into your gadgeteer harness to grant you temporary, limited flight. Activating or deactivating the jets requires a bonus action and, while active, you have a flying speed of 30 feet.\n\nThe jet pack last for 1 minute before deactivating. Once the jets have been activated, they can’t be activated again until you finish a short or long rest.\n\nYour jet pack’s speed increases to 40 feet at 5th level, 50 feet at 9th level, 60 feet at 13th level, 70 feet at 17th level, and 80 feet at 20th level." compset="CustomSpec" uniqueness="unique">
    <fieldval field="abDuration" value="1"/>
    <tag group="abRange" tag="Personal"/>
    <tag group="abDuration" tag="Minute"/>
    <tag group="AllowRCust" tag="cfgPRGadget"/>
    <tag group="abCategory" tag="GadgetCon"/>
    <tag group="Helper" tag="Tertiary"/>
    <tag group="User" tag="Activation"/>
    <bootstrap thing="xFly"></bootstrap>
    <eval phase="PostLevel" priority="10000"><![CDATA[~ If we're not enabled, get out now
doneif (field[abilActive].value = 0)
field[abValue2].value = #levelcount[Engineer]

if (field[abValue2].value >= 20) then
   hero.child[xFly].field[abValue].value = 80
elseif (field[abValue2].value >= 17) then
   hero.child[xFly].field[abValue].value = 70
elseif (field[abValue2].value >= 13) then
   hero.child[xFly].field[abValue].value = 60
elseif (field[abValue2].value >= 9) then
   hero.child[xFly].field[abValue].value = 50
elseif (field[abValue2].value >= 5) then
   hero.child[xFly].field[abValue].value = 40
else
   hero.child[xFly].field[abValue].value = 30
endif]]></eval>
    </thing>
  <thing id="cEngPRAutoThrust" name="Autothrusters" description="{i}{b}Prerequisite: {/b}Jet Pack{/i}\n\nYou can take the Dash and Disengage actions as a bonus action while your jet pack is active." compset="CustomSpec" uniqueness="unique">
