    <eval phase="Render" priority="10000"><![CDATA[field[abValue].value = #levelcount[Engineer]

if (field[abValue].value >= 13) then
   field[livename].text = field[thingname].text & " +3"
elseif (field[abValue].value >= 9) then
   field[livename].text = field[thingname].text & " +2"
else
   field[livename].text = field[thingname].text & " +1"
endif]]></eval>
    <exprreq message="5th level required."><![CDATA[#levelcount[Engineer] >= 5]]></exprreq>
    </thing>
  <thing id="cEngPRMim" name="Mimicker" description="{i}{b}Prerequisite: {/b}9th level{/i}\n\nYou create a device that attaches to your gadgeteer harness. Your mimicker casts a shadow that makes you appear to be standing in a place near your actual location, causing any creature to have disadvantage on attack rolls against you. If you take damage, the property ceases to function for 1 minute. Your mimicker is suppressed while you are incapacitated, restrained, or otherwise unable to move." compset="CustomSpec" uniqueness="unique">
    <fieldval field="trkMax" value="1"/>
    <fieldval field="abDuration" value="1"/>
