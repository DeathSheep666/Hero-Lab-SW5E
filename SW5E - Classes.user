    <eval phase="PostLevel">~ perform hero.assign[CondImm.pcnShocked]</eval>
    </thing>
  <thing id="cEngPRGadgetEng" name="Gadgeteer Engineering" description="Those engineers who choose the Gadgeteer discipline focus on the ability to craft reusable consumables and items that augment and complement their techcasting abilities." compset="CustomSpec" uniqueness="unique">
