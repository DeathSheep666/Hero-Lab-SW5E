  <thing id="cEngPRXGuard" name="Crossguard" description="You add an energy guard at the base of your modified lightsaber’s blade. While wielding your modified lightsaber, you gain a +1 bonus to your armor class against melee weapon attacks." compset="CustomSpec" uniqueness="unique">
