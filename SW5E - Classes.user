  <thing id="cXXXPRDjemSo" name="Djem So Form" description="Before the end of your next turn, you can add half your Wisdom or Charisma modifier (your choice, rounded down, minimum of +1) to one ability check or attack roll you make using Strength." compset="CustomSpec" uniqueness="unique">
    <fieldval field="abDuration" value="1"/>
