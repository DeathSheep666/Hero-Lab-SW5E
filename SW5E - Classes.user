  <thing id="cEngPRSongFlow" name="Song Flow" description="{i}{b}Prerequisite: {/b}11th level{/i}\n\nWhile playing an enhanced song, you can use your bonus action to change from one song to another." compset="CustomSpec" uniqueness="unique">
