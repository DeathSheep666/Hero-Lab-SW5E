    <exprreq message="5th level required."><![CDATA[#levelcount[Engineer] >= 5]]></exprreq>
    </thing>
  <thing id="cfgPRBiochem" name="Biochem Modifications" description="Configurable to expose the Biochem modification table." compset="Configure" uniqueness="unique">
