    <tag group="abRange" tag="Personal"/>
    <tag group="abDuration" tag="Instant"/>
    <tag group="abAction" tag="Bonus"/>
    </thing>
  <thing id="cSchPRCripStrk" name="Crippling Strike" description="When you hit a creature with a weapon attack, you can expend one superiority die to cripple the creature. You add the superiority die to the attack’s damage roll and the creature’s gains 1 slowed level until the end of their next turn." compset="CustomSpec" uniqueness="unique">
    <fieldval field="abDuration" value="1"/>
