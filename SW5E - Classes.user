    <tag group="abCategory" tag="FtrFigStyl"/>
    <tag group="SpecSource" tag="cHelpSct"/>
    <tag group="abCategory" tag="SctFigStyl"/>
    </thing>
  <thing id="cXXXPROnslaught" name="Onslaught Style" description="You are skilled at using your momentum to your advantage. You gain the following benefits:\n\n- You can take the Dash action as a bonus action. If you could already take the Dash action as a bonus action, you can instead take it as a reaction on your turn.\n- When you attempt to trip a creature, instead of making a Strength (Athletics) check, you can make an attack roll against the target’s AC. If you are wielding your weapon in two hands, you make this attack roll with advantage." compset="CustomSpec" uniqueness="unique">
    <tag group="SpecSource" tag="cHelpGrd"/>
    <tag group="Helper" tag="Secondary"/>
    <tag group="abCategory" tag="GrdFight"/>
    <tag group="abRange" tag="Personal"/>
    <tag group="abDuration" tag="Constant"/>
