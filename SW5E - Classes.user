  <thing id="cGrdPRChCauseHrm" name="Channel the Force: Cause Harm" description="As an action, you can expend a use of your Channel the Force to sap the life from a hostile creature you can see within 60 feet. That creature must make a Constitution saving throw. On a failed save, the creature takes necrotic damage equal to your guardian level + your Charisma modifier (minimum of one), or half as much on a successful one." compset="ClSpecial" summary="You can channel divine energy to fuel magical effects.">
    <fieldval field="abRange" value="60"/>
    <tag group="abRange" tag="Feet"/>
    <tag group="abDuration" tag="Instant"/>
    <tag group="abAction" tag="Action"/>
    <tag group="abSave" tag="aCON"/>
    <tag group="StandardDC" tag="aCHA"/>
    <eval phase="PostAttr" priority="10000"><![CDATA[field[abValue].value = #attrbonus1[aCHA]
field[abValue2].value = field[abValue].value + #levelcount[Guardian]

field[listname].text = field[thingname].text & " (" & field[abValue2].value & " damage)"

field[livename].text = field[thingname].text & " (" & field[abValue2].value & " damage)"]]></eval>
    </thing>
  <thing id="cGrdPRFrcEmpStrik" name="Force-Empowered Strikes" description="When you hit a creature with a melee weapon attack, you can expend force points to deal additional damage to the target, which is the same type as the weapon’s damage. The additional damage is 1d8 for each point spent in this way. You can’t deal more additional damage than the amount shown in the Focused Strikes column of the guardian table." compset="ClSpecial" summary="Expend a spell slot to deal extra radiant damage on melee attack.">
    <tag group="abAction" tag="Free" name="Free" abbrev="Free"/>
    <tag group="abDuration" tag="Instant" name="Instantaneous" abbrev="inst"/>
    <tag group="abRange" tag="Touch" name="Touch" abbrev="tch"/>
    <eval phase="Render" priority="10000" index="2"><![CDATA[        field[listname].text = field[thingname].text & " (" & int(field[xIndex].value + 1) & "d8)"
        field[livename].text = field[thingname].text & " (" & int(field[xCount].value + 1) & "d8)"]]></eval>
    </thing>
  <thing id="cGrdPRForcePure" name="Force Purity" description="The Force flowing through you makes you immune to poison and disease." compset="ClSpecial">
    <tag group="abRange" tag="Mile"/>
    <tag group="abDuration" tag="Constant"/>
    <eval phase="PostLevel" priority="10000">perform hero.assign[CondImm.pcnPoison]
perform hero.assign[CondImm.pcnDisease]</eval>
    </thing>
  <thing id="cGrdPRImpFES" name="Improved Force-Empowered Strikes" description="You are so in tune with the Force that all your melee weapon strikes carry the power of the Force with them. Whenever you hit a creature with a melee weapon attack, the creature takes an extra 1d8 damage. If you also use your Force-Empowered Strikes with an attack, you add this damage to the extra damage of your Force-Empowered Strikes. The damage is the same type as the weapon’s damage." compset="ClSpecial">
    <tag group="abRange" tag="Personal"/>
    <tag group="abDuration" tag="Constant"/>
    </thing>
  <thing id="cGrdPRCleanTouch" name="Cleansing Touch" description="You can use your action and expend a use of your Channel the Force ability to end one force power on yourself or on one willing creature that you touch." compset="ClSpecial">
    <tag group="abRange" tag="Touch"/>
    <tag group="abDuration" tag="Instant"/>
    <tag group="abAction" tag="Action"/>
    </thing>
  <thing id="cXXXPRAtaru" name="Ataru Form" description="As a part of the bonus action to adopt this form, you can leap up to 15 feet to an unoccupied space you can see." compset="CustomSpec" uniqueness="unique">
