  <thing id="cMonPRPureBody" name="Purity of Body" description="You are immune to disease and poison and resistant to poison damage." compset="ClSpecial" summary="You are immune to disease and poison.">
    <tag group="CondImm" tag="pcnDisease" name="diseased" abbrev="diseased"/>
