    <tag group="abCategory" tag="FtrFigStyl"/>
    <tag group="SpecSource" tag="cHelpSct"/>
    <tag group="abCategory" tag="SctFigStyl"/>
    </thing>
  <thing id="cXXXPRTwinBlade" name="Twin-Blade Style" description="You are skilled at fighting with double-bladed weapons. While you are wielding a weapon with the double property with which you are proficient, you gain the following benefits:\n\n- When you engage in Double-Weapon Fighting, you can add your ability modifier to the attack roll of your Double-Weapon Fighting attack.\n- When you attempt to shove a creature, instead of making a Strength (Athletics) check, you can make an attack roll against the target’s AC. If you are wielding your weapon in two hands, you make this attack roll with advantage." compset="CustomSpec" uniqueness="unique">
    <tag group="SpecSource" tag="cHelpGrd"/>
    <tag group="Helper" tag="Secondary"/>
    <tag group="abCategory" tag="GrdFight"/>
    <tag group="abRange" tag="Personal"/>
