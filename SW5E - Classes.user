    <tag group="abDuration" tag="Constant"/>
    <tag group="abRange" tag="Personal"/>
    <tag group="Usage" tag="Round"/>
    </thing>
  <thing id="cMonPREchaniOrder" name="Echani Order" description="Monks of the Echani Order train relentlessly both with and without their weapons, to the point where the weapon becomes an extension of the body. Founded on a mastery of sword fighting, the tradition has expanded to include many different weapons." compset="CustomSpec" uniqueness="unique">
